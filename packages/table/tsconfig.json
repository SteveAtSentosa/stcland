{
  "extends": "../config/tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
   "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "allowArbitraryExtensions": true,
    "baseUrl": ".",
    "paths": {
       "@/*": ["src/*"],
      "@storybook/*": ["../storybook*"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
